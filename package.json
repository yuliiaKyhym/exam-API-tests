{
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "allure-commandline": "^2.21.0",
    "cypress": "^12.7.0"
  },
  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^2.35.2",
